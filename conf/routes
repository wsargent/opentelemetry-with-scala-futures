GET     /future     controllers.HomeController.futureGet

GET     /broken     controllers.HomeController.brokenGet

GET     /fixed    controllers.HomeController.fixedGet

GET     /context    controllers.HomeController.contextGet

GET     /cat        controllers.HomeController.cat

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)